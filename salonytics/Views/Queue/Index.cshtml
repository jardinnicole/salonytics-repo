@model IEnumerable<salonytics.Models.Entities.Queue>

@{
    ViewData["Title"] = "Queue List";
}

<h1>@ViewData["Title"]</h1>

<p>
    <a href="@Url.Action("Create", "Queue")" class="btn btn-primary">Create New</a>
</p>

<table class="table">
    <thead>
        <tr>
            <th>Queue Id</th>
            <th>Appointment</th>
            <th>Position</th>
            <th>Status</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.QueueId</td>
                <td>@item.Appointment?.FullName</td>
                <td>@item.Position</td>
                <td>@item.Status</td>
                <td>
                    <a href="@Url.Action("Details", "Queue", new { id = item.QueueId })" class="btn btn-info">Details</a>
                </td>
            </tr>
        }
    </tbody>
</table>
 